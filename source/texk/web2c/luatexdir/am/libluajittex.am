## libluajit
##
LUATEXJIT_INCLUDES = $(LUAJIT_INCLUDES) $(LUAJIT_DEFINES)
LUATEXJIT_LIBS = $(LUAJIT_LIBS)
LUATEXJIT_DEPEND = $(LUAJIT_DEPEND)


## libluatex
##
EXTRA_LIBRARIES += libluajittex.a

libluajittex_a_DEPENDENCIES = libjitff.a

libluajittex_a_CPPFLAGS = $(AM_CPPFLAGS) $(ZLIB_INCLUDES) $(LIBPNG_INCLUDES)
libluajittex_a_CPPFLAGS += $(POPPLER_INCLUDES) -I$(srcdir)/libmd5
libluajittex_a_CPPFLAGS += -DpdfTeX -DLuajitTeX -DLUAJIT_ENABLE_LUA52COMPAT -I$(srcdir)/luatexdir
libluajittex_a_CPPFLAGS += $(LUATEXJIT_INCLUDES)
libluajittex_a_CPPFLAGS += -DSYNCTEX_ENGINE_H='<synctex-luatex.h>' -I$(srcdir)/synctexdir
libluajittex_a_CXXFLAGS = # $(WARNING_CXXFLAGS)

## compatibility between poppler versions <=0.24.0 and >-0.25.0
libluajittex_a_CPPFLAGS += -I$(srcdir)/luatexdir/fake
EXTRA_DIST += luatexdir/fake/StructTreeRoot.h

